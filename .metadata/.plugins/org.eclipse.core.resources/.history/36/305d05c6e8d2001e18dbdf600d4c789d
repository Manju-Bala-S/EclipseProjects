package com.manju.ChessGame;

public class ChessMan {

	private char color;
	private String id;
	private String position;
	private char type;
	public char getColor() {
		return color;
	}
	public void setColor(char color) {
		this.color = color;
	}
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	
	public char getType() {
		return type;
	}
	public void setType(char type) {
		this.type = type;
	}
	
}
